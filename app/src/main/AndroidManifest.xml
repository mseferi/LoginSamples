<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginsamples">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_cmyk"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.example.loginsamples.screens.RegisterActivity" />
        <activity android:name="com.example.loginsamples.screens.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.loginsamples.screens.CustomPrintActivity">

        </activity>
    </application>

</manifest>